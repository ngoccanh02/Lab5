
<div class="product__details__rating">
    @for (int i = 0; i < ViewBag.Avg; i++)
    {
    <i class='fa fa-star' style='color: #f3da35'></i>
    }
    @for (int i = 0; i < 5 - ViewBag.Avg; i++)
    {
    <i class='fa fa-star' style='color:#cab7e1'></i>
    }
    <span>@ViewBag.count</span>
</div>
